{
  "name": "client",
  "version": "0.0.1",
  "main": "built/index.js",
  "author": "Andrew Hickey <andrewhickey@live.co.uk>",
  "license": "MIT",
  "scripts": {
    "test": "jest --env node",
    "coverage": "jest --env node --coverage",
    "precommit": "lint-staged",
    "pretty": "prettier --write src/**/*.ts"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "rootDir": "src",
    "testRegex": ".*(\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "nock": "^9.0.14",
    "popsicle": "^9.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-test-renderer": "^15.5.4",
    "@types/recompose": "^0.24.1",
    "@types/redux-actions": "^1.2.8",
    "@types/redux-mock-store": "^0.0.11",
    "@types/redux-promise": "^0.5.26",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1",
    "react-test-renderer": "^15.5.4",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.2.3",
    "ts-jest": "^20.0.14",
    "typescript": "^2.5.2"
  }
}
